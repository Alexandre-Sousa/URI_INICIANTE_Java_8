import java.util.Scanner;

public class Main {

	public static void main(String args[]) {
		
		Scanner entrada = new Scanner (System.in);
		
		double R = entrada.nextDouble();
		
		double PI = 3.14159;
		
		double A = PI * (R * R);
				
		System.out.format("A=%.4f%n", A);
		
		entrada.close();

	}

}
