import java.util.Scanner;
public class Main {
	public static void main (String[] args) {
		Scanner type = new Scanner (System.in);
		int a = type.nextInt();
		int b = type.nextInt();
		int c = type.nextInt();
		int d = type.nextInt();
		int diferenca = ((a * b) - (c * d));
		System.out.println("DIFERENCA = " + diferenca);
		type.close();
	}
}
