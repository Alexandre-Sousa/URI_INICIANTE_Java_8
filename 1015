import java.util.Scanner;

public class Main {
	
	public static void main (String[] args) {
		
		Scanner type = new Scanner (System.in);
		
		double x1 = type.nextDouble();
		double y1 = type.nextDouble();
		double x2 = type.nextDouble();
		double y2 = type.nextDouble();
		
		double dist = Math.sqrt(Math.pow(x2 - x1, 2) + Math.pow(y2 - y1, 2));
		
		System.out.format("%.4f%n", dist);
		
		type.close();
	
	}
	
}
