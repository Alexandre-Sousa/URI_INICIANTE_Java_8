import java.util.Scanner;

public class Main {
	public static void main (String[] args) {
		
		Scanner type = new Scanner (System.in);
		
		int value, i, pot;
		
		value = type.nextInt();
		
		for (i = 2; i <= value; i = i +2) {
			pot = (int) Math.pow(i, 2);
			System.out.println(i + "^2 = " + pot);
		}
		
		type.close();
		
	}
}
