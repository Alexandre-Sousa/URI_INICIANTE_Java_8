import java.util.Scanner;

public class Main {
	public static void main (String[] args) {
		
		Scanner type = new Scanner (System.in);
		int value, i, prod;
		
		value = type.nextInt();
		
		for (i = 1; i <= 10; i++) {
			prod = i * value;
			System.out.println(i + " x " + value + " = " + prod);
		}		
		
		type.close();		
	}
}
