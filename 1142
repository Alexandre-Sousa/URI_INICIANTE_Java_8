import java.util.Scanner;

public class Main{
	public static void main(String[] args) {
		
		Scanner input = new Scanner(System.in);
		
		int value, i, j, cont = 1;
		
		value = input.nextInt();
		
		for (i = 0; i < value; i++) {
			for (j = 0; j < 3; j++) {
				System.out.printf("%d ", cont);
				cont++;
			}
			cont++;
			System.out.println("PUM");
		}
			
		input.close();
		
	}
}
